{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-console": false,
        "only-arrow-functions": false,
        "no-shadowed-variable": false,
        "app/no-options": false,
        "triple-equals": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}